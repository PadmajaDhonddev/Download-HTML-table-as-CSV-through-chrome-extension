{
	"name": "Download table as CSV",
	"description": "Download HTML tables in Excel style CSV format.",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": ["contextMenus", "activeTab"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},"browser_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"32": "icons/32x32.png",
		"64": "icons/64x64.png",
		"128": "icons/128x128.png"
	}
}
